{
  "manifest_version": 3,
  "name": "Quiz Solver",
  "version": "1.3",
  "description": "Quiz Solver with Chat GPT is a powerful Chrome plugin that uses AI technology to help you solve quizzes quickly and easily",
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}